bool _bari(byte _eli)                    // udda = coloumn   // adda = row
{ 
  bool _chukki;
  byte _uli, _gunita, _etko, _achu;
  byte _tiliha , _agala, _saalu;
  byte wrow, _ycol, _ella;

  if(_eli == 0)                           //deerga
  {
    _eli = 247;
  }

  _uli = _eli / 11;
  _gunita = _eli % 11;

  if(_gunita == 0)
  {
    _uli = _uli - 1;
    _gunita = 11;
  }

  _ycol = _colkan;
  wrow = _rowkan;


  if(_eli <= 231)
  {
    if(_gunita == 3 || _gunita == 4)
    {
      _etko = pgm_read_byte(_ki + _uli);
      _tiliha = pgm_read_byte(_padagudusu + _etko);
    }
    else
    {
    _etko = pgm_read_byte(_kk + _uli);
    _tiliha = pgm_read_byte(_kainda + _etko);
    wrow = wrow +2;
    }
    
  }

  else
  {
    _gunita = _eli - 232;
    _etko = pgm_read_byte(_aa + _gunita);
    _tiliha = pgm_read_byte(_uligalu + _etko);
  }

  _saalu = _tiliha & 0x0F;
  _achu = _etko;
  _ella =  ((_tiliha >> 4) & 0x0F)* _saalu ;
  
 
  for (_agala = 0; _agala < _ella; _agala++)
  {
    if( _agala % 8 == 0)
    {
      _etko = _etko + 1;
      if(_eli <= 231)
        {
           if(_gunita == 3 || _gunita == 4)
             {
               _tiliha = pgm_read_byte(_padagudusu + _etko);
             }
           else
             {
               _tiliha = pgm_read_byte(_kainda + _etko);
             }
        }
      else
      {
        _tiliha = pgm_read_byte(_uligalu + _etko);
      }
    }


    
    _chukki = bitRead(_tiliha, 7 - (_agala % 8));


    
    if(_agala !=0)
    {
    if(_agala % _saalu == 0)
    {
      wrow = wrow + 1;
      _ycol = _colkan;
    }
    }
    display.drawPixel( _ycol, wrow, _chukki);
    _ycol = _ycol +1;
  }

  if(_gunita == 3 || _gunita == 4)
             {
               if(_uli == 12)
               {
                 _colkan = _colkan- 2;
                 _kombu(_achu);
                 _colkan = _colkan + 2;
               }
             }
  display.display();




  
  if(_eli <= 231)
  {
    if(_gunita != 3 || _gunita !=4)
    {
      _kagunita(_achu, _gunita);
    }
  }
  _colkan = _colkan + _saalu + 1 ;
  if(_eli <= 231)
  {
   if(_gunita == 4 ||_gunita == 8 ||_gunita == 11 )
   {
     return 1;
   }
   else
   {
    return 0;
   }
  }
  else
  {
    return 0;
  }
}


