              _colkan = _colkan + 1;
              }

              _achu = 0;
            }
            else
            {
             _achu = 1;
            }
            }       
   else
   {
     _achu = 0;
     if(_yavudu == pgm_read_byte(_kk + 2) || _yavudu == pgm_read_byte(_kk + 13))
            {
              if(_ena == 0)
              {
