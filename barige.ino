   if ( _yavudu == pgm_read_byte(_kk + 3) || _yavudu == pgm_read_byte(_kk + 4) ||  _yavudu == pgm_read_byte(_kk + 6) || _yavudu == pgm_read_byte(_kk + 11) || _yavudu == pgm_read_byte(_kk + 15))
          {
            if(_ena == 1)
            {
              _achu = 0;
              if( _yavudu == pgm_read_byte(_kk + 6))
              {
               wcol = wcol + 3;
               _ele = 5;
               _colkan = _colkan + 1;
              }
              else
              {
              wcol = wcol + 4;
