           if ( _yavudu == 90 || _yavudu == 100 )
           {
              _keele(_yavudu,1);
           } 
           else
           {
            _kombu(_yavudu);
            _keele(_yavudu,1);
           }
      
}


void _yivattu(byte _yavudu)
{
  byte wrow = _rowkan;
  byte wcol = _colkan;
  _talakattu(_yavudu,1);
  _rowkan = wrow;
  _colkan = wcol;
  wrow = _colkan;
  wcol = _rowkan;  
  byte _ele = pgm_read_byte(_kainda + _yavudu);
  _ele = _ele & 0x0F;

    wcol = wcol + 10;
    wrow = wrow +_ele - 1;
     display.drawPixel(wrow,wcol,1);
     display.drawPixel(wrow+1,wcol,1);
     display.drawPixel(wrow+2,wcol,1);
     display.drawPixel(wrow+3,wcol-1,1);
     display.drawPixel(wrow+3,wcol-2,1);
     display.drawPixel(wrow+2,wcol-3,1);
     display.drawPixel(wrow+3,wcol-4,1);
     display.drawPixel(wrow+3,wcol-5,1);
     _colkan = _colkan + 3;
     display.display();
}



void _barivattu(byte _eli)                    // udda = coloumn   // adda = row
{ 
  bool _chukki , _kodi;
  byte _udda,_ella,_ene,_agala,wrow,wcol;
  wcol = _colkan - 1;
  wrow = _rowkan + 6;
  byte _tiliha = pgm_read_byte(_vattuligalu + _eli);
  if(_tiliha == 0x88)
  {
     wrow = wrow + 3;
     display.drawPixel(wcol,wrow,1);
     display.drawPixel(wcol - 1,wrow,1);
     wrow = wrow+1;
     for(_agala = 0; _agala <5 ; _agala++)
     {
       display.drawPixel(wcol - _agala,wrow,1);
     }
     _udda = 0;
  }

  else if(_tiliha == 0x89)
  {
     wrow = wrow + 1;
     wcol = wcol + 1;
     display.drawPixel(wcol,wrow,1);
     display.drawPixel(wcol,wrow + 1,1);
     display.drawPixel(wcol - 1,wrow + 2,1);
     display.drawPixel(wcol - 2,wrow + 3,1);
     display.drawPixel(wcol - 3,wrow + 3,1);
     display.drawPixel(wcol - 4,wrow + 3,1);
     _colkan = _colkan+2;
  }

  else
  {
  _udda = _tiliha & 0x07;
  
  if((_tiliha & 0x08) == 0x08)
  {
    wrow = wrow + 2;
    wcol = wcol - 1;
    _ella = 3* _udda;
    _kodi = 1;
  }
  else
  {
  _ella = 5 * _udda;
  _kodi = 0;
  }
  _ene = _eli;

 
  for (_agala = 0; _agala < _ella; _agala++)
  {
    if( _agala % 8 == 0)
    {
      _ene = _ene + 1;
      _tiliha = pgm_read_byte(_vattuligalu + _ene);
    }


    _chukki = bitRead(_tiliha, 7 - (_agala % 8));


    if(_agala !=0)
    {
    if(_agala % _udda == 0)
    {
      wrow = wrow + 1;
      wcol = _colkan - 1;
      if(_kodi  == 1)
      {
        wcol = wcol - 1;
      }
    }
    }
    display.drawPixel(wcol,wrow,_chukki);
    wcol = wcol +1;
  }
  _colkan = _colkan + _udda - 1;
  }


  display.display();
}


void _surali()
{
  display.drawPixel(_colkan - 1,_rowkan,1);
  display.drawPixel(_colkan - 2,_rowkan,0);
  display.drawPixel(_colkan - 2,_rowkan + 1,1);
  display.drawPixel(_colkan - 4,_rowkan + 2,1);
  display.drawPixel(_colkan - 3,_rowkan + 2,1);
  display.drawPixel(_colkan - 2,_rowkan + 2,1);
  display.drawPixel(_colkan - 1,_rowkan + 2,1);
  display.drawPixel(_colkan - 2,_rowkan + 3,1);
  display.drawPixel(_colkan    ,_rowkan + 3,1);
  display.drawPixel(_colkan - 1,_rowkan + 4,1);
  display.display();
  _colkan = _colkan + 2;
}
